
@{
    ViewBag.Title = "Index";
    //Layout = null;
    Layout = "~/Views/Shared/_Layout.cshtml";

}
@model List<LikeSport.Web.Models.ActivityGroupViewModel>
<link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/animate.css/3.4.0/animate.min.css">
<div class="section-header mheader">
    <div class="btn-group">
        <button type="button" class="button-label btn ink-reaction btn-flat dropdown-toggle" data-toggle="dropdown" aria-expanded="true">
            Groups
            <sup class="badge style-success-screen" style="display: none; margin: 2px; background: #008489 !important;    padding: 6px 4px 0px 2px;" id="amount"></sup>
            <i class="fa fa-angle-down text-default-green"></i>
        </button>
        <div class="dropdown-menu animation-zoom category-menu" role="menu">
            <div style="padding: 20px">
                <div class="row" style="display: inline-flex;">
                    @if (Model.Any())
                    {
                        var loop = Model.Count / 6;
                        var skip = 0;
                        for (int i = 0; i < loop; i++)
                        {
                            <div class="col-md-12">
                                @foreach (var item in Model.Skip(skip).Take(6))
                                {

                                    <p>
                                        <label class="checkbox-inline checkbox-styled">
                                            <input onclick="onchecking(this)" type="checkbox" value="@item.Id"><span>@item.ActivityGroupName</span>
                                        </label>
                                    </p>

                                }
                            </div>
                            skip += 6;
                        }

                    }


                </div>
                <div class="row">
                    <div class="col-sm-6"><button style="font-size: 10px!important;" type="button" id="btnCancel" class="btn btn-block ink-reaction btn-flat btn-danger">Cancel</button></div>
                    <div class="col-sm-6 pull-right">
                        <button type="button" style="font-size: 10px!important;" id="btnApply" onclick="_fnApply()" class="btn btn-block ink-reaction btn-flat btn-primary ">Apply</button>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div><!--end .section-header -->
<section id="section">

